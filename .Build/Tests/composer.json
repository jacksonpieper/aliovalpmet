{
    "name": "schnitzler/templavoila",
    "description": "",
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "php": ">=7.0.0",
        "typo3/cms": "8.6.*",
        "typo3/testing-framework": "1.0.*",
        "phpunit/phpcov": "3.1.0",
        "codeclimate/php-test-reporter": "^0.4.4"
    },
    "autoload": {
        "psr-4": {
            "Schnitzler\\System\\Localization\\": "src/System.Localization/src/",
