            "Schnitzler\\TemplaVoila\\View\\": "src/TemplaVoila.View/src/",
            "Schnitzler\\TYPO3\\CMS\\Backend\\": "src/TYPO3.CMS.Backend/src/",
            "Schnitzler\\TYPO3\\CMS\\Core\\": "src/TYPO3.CMS.Core/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schnitzler\\System\\Localization\\Tests\\Unit\\": "src/System.Localization/tests/unit/",
            "Schnitzler\\System\\Localization\\Tests\\Functional\\": "src/System.Localization/tests/functional/",
            "Schnitzler\\System\\Runtime\\Tests\\Unit\\": "src/System.Runtime/tests/unit/",
            "Schnitzler\\System\\Runtime\\Tests\\Functional\\": "src/System.Runtime/tests/functional/",
            "Schnitzler\\System\\Data\\Tests\\Unit\\": "src/System.Data/tests/unit/",
            "Schnitzler\\System\\Data\\Tests\\Functional\\": "src/System.Data/tests/functional/",
            "Schnitzler\\System\\IO\\Tests\\Unit\\": "src/System.IO/tests/unit/",
            "Schnitzler\\System\\IO\\Tests\\Functional\\": "src/System.IO/tests/functional/",
            "Schnitzler\\System\\Traits\\Tests\\Unit\\": "src/System.Traits/tests/unit/",
            "Schnitzler\\System\\Traits\\Tests\\Functional\\": "src/System.Traits/tests/functional/",
