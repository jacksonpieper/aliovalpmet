            "Schnitzler\\TemplaVoila\\Core\\Tests\\Functional\\": "src/TemplaVoila.Core/tests/functional/",
            "Schnitzler\\TemplaVoila\\Data\\Tests\\Unit\\": "src/TemplaVoila.Data/tests/unit/",
            "Schnitzler\\TemplaVoila\\Data\\Tests\\Functional\\": "src/TemplaVoila.Data/tests/functional/",
            "Schnitzler\\TemplaVoila\\Security\\AccessControl\\Tests\\Unit\\": "src/TemplaVoila.Security.AccessControl/tests/unit/",
            "Schnitzler\\TemplaVoila\\Security\\AccessControl\\Tests\\Functional\\": "src/TemplaVoila.Security.AccessControl/tests/functional/",
            "Schnitzler\\TemplaVoila\\Security\\Permissions\\Tests\\Unit\\": "src/TemplaVoila.Security.Permissions/tests/unit/",
            "Schnitzler\\TemplaVoila\\Security\\Permissions\\Tests\\Functional\\": "src/TemplaVoila.Security.Permissions/tests/functional/",
            "Schnitzler\\TemplaVoila\\UI\\Tests\\Unit\\": "src/TemplaVoila.UI/tests/unit/",
            "Schnitzler\\TemplaVoila\\UI\\Tests\\Functional\\": "src/TemplaVoila.UI/tests/functional/",
            "Schnitzler\\TemplaVoila\\View\\Tests\\Unit\\": "src/TemplaVoila.View/tests/unit/",
            "Schnitzler\\TemplaVoila\\View\\Tests\\Functional\\": "src/TemplaVoila.View/tests/functional/",
            "Schnitzler\\TYPO3\\CMS\\Backend\\Tests\\Unit\\": "src/TYPO3.CMS.Backend/tests/unit/",
            "Schnitzler\\TYPO3\\CMS\\Backend\\Tests\\Functional\\": "src/TYPO3.CMS.Backend/tests/functional/",
            "Schnitzler\\TYPO3\\CMS\\Core\\Tests\\Unit\\": "src/TYPO3.CMS.Core/tests/unit/",
            "Schnitzler\\TYPO3\\CMS\\Core\\Tests\\Functional\\": "src/TYPO3.CMS.Core/tests/functional/"
        }
    },
    "replace": {
        "templavoila": "self.version",
        "typo3-ter/templavoila": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/templavoila ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/templavoila"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
