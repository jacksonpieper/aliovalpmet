{
  "name": "schnitzler/templavoila",
  "description": "",
  "license": ["GPL-2.0+"],
  "require": {
    "php": ">=7.0.0",
    "typo3/cms": "dev-master",
    "nimut/testing-framework": "1.1.3"
  },
  "autoload": {
    "psr-4": {
      "Schnitzler\\Templavoila\\": ["Classes/", "Interface/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Schnitzler\\Templavoila\\": "Tests/"
    }
  },
  "replace": {
    "templavoila": "self.version",
    "typo3-ter/templavoila": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "preferred-install": {
      "typo3/cms": "source",
      "*": "dist"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/templavoila ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/templavoila"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
