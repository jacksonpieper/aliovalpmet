{
    "name": "schnitzler/templavoila",
    "type": "typo3-cms-extension",
    "description": "One of many availabe template engines for TYPO3 CMS",
    "homepage": "http://forge.typo3.org/projects/extension-templavoila/",
    "license": [
        "GPL-2.0+"
    ],
    "autoload": {
        "psr-4": {
            "Schnitzler\\Templavoila\\": "Classes/",
            "Schnitzler\\System\\Localization\\": "src/System.Localization/src/",
            "Schnitzler\\System\\Data\\": "src/System.Data/src/",
            "Schnitzler\\System\\Traits\\": "src/System.Traits/src/",
            "Schnitzler\\TemplaVoila\\Console\\": "src/TemplaVoila.Console/src/",
            "Schnitzler\\TemplaVoila\\ContextMenu\\": "src/TemplaVoila.ContextMenu/src/",
            "Schnitzler\\TemplaVoila\\Controller\\": "src/TemplaVoila.Controller/src/",
            "Schnitzler\\TemplaVoila\\Data\\": "src/TemplaVoila.Data/src/",
            "Schnitzler\\TemplaVoila\\Security\\Permissions\\": "src/TemplaVoila.Security.Permissions/src/",
            "Schnitzler\\TemplaVoila\\UI\\": "src/TemplaVoila.UI/src/",
            "Schnitzler\\TYPO3\\CMS\\Backend\\": "src/TYPO3.CMS.Backend/src/",
            "Schnitzler\\TYPO3\\CMS\\Core\\": "src/TYPO3.CMS.Core/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schnitzler\\Templavoila\\": "Tests/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "psr/container": "1.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1.1",
        "clue/phar-composer": "1.0.0"
    },
    "replace": {
        "templavoila": "self.version",
        "typo3-ter/templavoila": "self.version"
    }
}
